{
  "name": "node-lief",
  "version": "0.1.0-pre.4",
  "description": "Node.js bindings for LIEF - Library to Instrument Executable Formats",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "install": "echo 'Skipping automatic build - use `pnpm build` to build from source.'",
    "build": "pnpm build:lief && node-gyp rebuild",
    "build:lief": "node scripts/build-lief.js",
    "clean": "rm -rf build",
    "prebuildify": "prebuildify --napi --strip",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"lib/**/*.{js,ts,d.ts}\"",
    "format:check": "prettier --check \"lib/**/*.{js,ts,d.ts}\""
  },
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "node-gyp": "^10.0.0",
    "prebuildify": "^6.0.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "elf",
    "pe",
    "macho",
    "binary",
    "parsing",
    "executable",
    "dwarf",
    "pdb"
  ],
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Piebald-AI/node-lief"
  }
}
